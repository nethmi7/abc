  </servlet>
  <servlet>
  	<servlet-name>PatientLogController</servlet-name>
  	<display-name>PatientLogController</display-name>
  	<description></description>
  	<servlet-class>com.hirucode.abc.controller.PatientLogController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>WorkerController</servlet-name>
  	<display-name>WorkerController</display-name>
  	<description></description>
  	<servlet-class>com.hirucode.abc.controller.WorkerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PaymentController</servlet-name>
  	<display-name>PaymentController</display-name>
  	<description></description>
  	<servlet-class>com.hirucode.abc.controller.PaymentController</servlet-class>
  </servlet>
