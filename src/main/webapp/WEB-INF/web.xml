  	<servlet-name>TestResultController</servlet-name>
  	<display-name>TestResultController</display-name>
  	<description></description>
  	<servlet-class>com.hirucode.abc.controller.TestResultController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AdminLoginController</servlet-name>
  	<display-name>AdminLoginController</display-name>
  	<description></description>
  	<servlet-class>com.hirucode.abc.controller.AdminLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PatientController</servlet-name>
  	<url-pattern>/Patient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>PatientController</servlet-name>
	<url-pattern>/patient</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AppointmentController</servlet-name>
  	<url-pattern>/Appointment</url-pattern>
